{
    "[latex]": {
        "editor.formatOnSave": false,
    },
    "python.defaultInterpreterPath": "/Library/Frameworks/Python.framework/Versions/3.12/bin/python3",
    "editor.fontSize": 16,
    "settingsSync.keybindingsPerPlatform": false,
    "editor.mouseWheelZoom": true,
    "notebook.markup.fontSize": 20,
    "cSpell.enabled": true,
    "cSpell.language": "en,ru",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "latex-workshop.latexindent.path": "/Library/TeX/texbin/latexindent",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, TabWidth: 4, UseTab: Never, ColumnLimit: 80 }",
    "notebook.diff.enablePreview": false,
    "notebook.dragAndDropEnabled": false,
    "notebook.lineNumbers": "on",
    "workbench.preferredDarkColorTheme": "GitHub Light",
