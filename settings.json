    "C_Cpp.errorSquiggles": "disabled",
    "cmake.showConfigureWithDebuggerNotification": false,
    "clangd.arguments": [
        "--background-index",
        "--enable-config",
    ],
    "clangd.checkUpdates": true,
    "clangd.onConfigChanged": "restart",
    "C_Cpp.intelliSenseEngine": "disabled",
    "workbench.editorAssociations": {
        "*.md": "vscode.markdown.preview.editor"
    },
    "redhat.telemetry.enabled": true,
    "workbench.colorTheme": "Vim Dark Medium",
    "notebook.editorOptionsCustomizations": {
        "editor.tabSize": 4,
        "editor.indentSize": 4,
        "editor.insertSpaces": true,
        "editor.detectIndentation": true
    },
    "clangd.path": "/Users/flamedragon/Library/Application Support/Code/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/19.1.2/clangd_19.1.2/bin/clangd",
    "comment-divider.length": 60,
    "editor.tabSize": 2,
    "cSpell.userWords": [
        "Eigen",
        "sqlmock",
        "unmap"
    ],
    "jupyter.askForKernelRestart": false,
    "tabnine.experimentalAutoImports": true,
    "editor.formatOnSaveMode": "modifications",
    "clangd.fallbackFlags": [
        "\"-I${workspaceFolder}/si-single-node/transactions\", \"-I${workspaceFolder}/si-single-node/\"",
        "\"-I/usr/include/llvm-17\""
